<sv:node sv:name="logoutService" esv:merge="insert" esv:location="root" xmlns:sv="http://www.jcp.org/jcr/sv/1.0" xmlns:esv="http://www.onehippo.org/jcr/xmlimport">
  <!--
     Load the logoutService before the root plugin, since the latter depends on the former.
  -->
  <sv:property sv:name="jcr:primaryType" sv:type="Name">
    <sv:value>frontend:plugin</sv:value>
  </sv:property>
  <sv:property sv:name="plugin.class" sv:type="String">
    <sv:value>org.hippoecm.frontend.plugins.logout.ConsoleLogoutService</sv:value>
  </sv:property>
</sv:node>